{
  "description": "Download files from selected area without prompt",
  "manifest_version": 2,
  "name": "SelectionDownloader",
  "version": "0.2",
  "homepage_url": "https://github.com/Eivy/SelectionDownloader",
  "icons": {
    "16": "icons/collection.svg",
    "48": "icons/collection.svg",
    "96": "icons/collection.svg"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [{
    "matches": ["*://*/*"],
    "js": ["activeTab.js"]
  }],
  "options_ui": {
    "page": "options.html"
  },
  "permissions": [
    "menus"
    ,"activeTab"
    ,"downloads"
    ,"storage"
  ]
}
